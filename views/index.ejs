<%- include('header'); -%>

<div class="" style="  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  overflow: auto;
  background: #70cbd0;">

  <div class="m-5">
    <br>
    <br>
    <br>
    <div class="card pt-4 ps-2 pe-2 pb-2 mb-5   rounded-3 shadow bg-gray">

      <h1 class="ms-5">Western Teachers</h1>

      <div class="row mt-1 ms-5 me-5 mb-5">
        <% for (var i = 0; i < westernTeachers.length; i++) { %>
        <div class="col-sm-4 col-md-3 col-lg-2 m-1 rounded-3 shadow bg-primary" style="border: 0px solid">
          <a class="dropdown-item" style="font-size:1.5rem; color:white" href="#"><%= westernTeachers[i] %></a>
        </div>
        <% } %>
      </div>
    </div>

    <div class="card pt-4 ps-2 pe-2 pb-2 mb-5   rounded-3 shadow bg-gray">


      <h1 class="ms-5">Korean/Vietnamese Teachers</h1>
      <div class="row mt-1 ms-5 me-5 mb-5">
        <% for (var i = 0; i < koreanVietnameseTeachers.length; i++) { %>
        <div class="col-sm-4 col-md-3 col-lg-2 m-1 rounded-3 shadow bg-blue" style="border: 0px solid">
          <a class="dropdown-item " style="font-size:1.5rem; color:white" href="#"><%= koreanVietnameseTeachers[i] %></a>
        </div>
        <% } %>
      </div>
    </div>
  </div>

</div>



<%- include('footer'); -%>
