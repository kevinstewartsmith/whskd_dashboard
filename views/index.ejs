<%- include('partials/header'); -%>

<div class="" style="  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  overflow: auto;
  background: #70cbd0;">

  <div class="m-5">
    <br>
    <br>
    <br>

<!-- test -->
    <div class="card pt-4 ps-2 pe-2 pb-2 mb-5   rounded-3 shadow bg-gray">


      <h1 class="ms-5">Western Teachers</h1>
      <div class="row mt-1 ms-5 me-0 mb-5">
        <% for (var i = 0; i < westernTeachers.length; i++) { %>

          <!-- <a class="dropdown-item" style="font-size:1.5rem; color:white" href="#"><%= koreanVietnameseTeachers[i] %></a> -->
          <a type="button " href="/teachers/<%= westernTeachers[i] %>" class="btn btn-primary col-sm-4 col-md-3 col-lg-2 m-1 rounded-3 shadow bg-navy" style="font-size:1.5rem; text-align: left; text-indent: 10px">      <%= westernTeachers[i] %></a>

        <% } %>
      </div>
    </div>




    <div class="card pt-4 ps-2 pe-2 pb-2 mb-5   rounded-3 shadow bg-gray">


      <h1 class="ms-5">Korean/Vietnamese Teachers</h1>
      <div class="row mt-1 ms-5 me-0 mb-5">
        <% for (var i = 0; i < koreanVietnameseTeachers.length; i++) { %>

          <!-- <a class="dropdown-item" style="font-size:1.5rem; color:white" href="#"><%= koreanVietnameseTeachers[i] %></a> -->
          <a type="button " href="/teachers/<%= koreanVietnameseTeachers[i] %>" class="btn btn-primary col-sm-4 col-md-3 col-lg-2 m-1 rounded-3 shadow bg-orange" style="font-size:1.5rem; text-align: left; text-indent: 10px">      <%= koreanVietnameseTeachers[i] %></a>

        <% } %>
      </div>
    </div>

  </div>

</div>



<%- include('partials/footer'); -%>
